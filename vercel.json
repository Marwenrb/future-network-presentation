{
  "version": 2,
  "builds": [
    {
      "src": "future-network-presentation.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/future-network-presentation.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "outputDirectory": ".",
  "public": true
} 